{
  "name": "react-library-example",
  "version": "1.0.0",
  "description": "Simple example of react library",
  "author": "",
  "license": "",
  "repository": "",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "typings": "dist/index",
  "scripts": {
