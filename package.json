    "onchange": "6.0.0",
    "postcss": "^8.2.6",
    "prettier": "1.18.2",
    "react": "^16.8.6",
    "react-docgen-typescript": "^1.12.5",
    "react-docgen-typescript-loader": "3.1.0",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
